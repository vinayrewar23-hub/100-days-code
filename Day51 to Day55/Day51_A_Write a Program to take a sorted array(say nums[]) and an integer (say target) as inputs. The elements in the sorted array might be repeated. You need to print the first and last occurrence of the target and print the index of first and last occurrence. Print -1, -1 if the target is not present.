#include <stdio.h>

int firstOcc(int a[], int n, int target) {
    int l = 0, r = n - 1, ans = -1;
    while (l <= r) {
        int mid = (l + r) / 2;
        if (a[mid] == target) {
            ans = mid;
            r = mid - 1;  // go left
        } else if (a[mid] > target) {
            r = mid - 1;
        } else {
            l = mid + 1;
        }
    }
    return ans;
}

int lastOcc(int a[], int n, int target) {
    int l = 0, r = n - 1, ans = -1;
    while (l <= r) {
        int mid = (l + r) / 2;
        if (a[mid] == target) {
            ans = mid;
            l = mid + 1;  // go right
        } else if (a[mid] > target) {
            r = mid - 1;
        } else {
            l = mid + 1;
        }
    }
    return ans;
}

int main() {
    int n, target;
    scanf("%d", &n);

    int nums[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    scanf("%d", &target);

    int first = firstOcc(nums, n, target);
    int last  = lastOcc(nums, n, target);

    printf("%d %d\n", first, last);

    return 0;
}
