#include <stdio.h>

int main() {
    int n, pos, element;
    scanf("%d", &n);
    
    int arr[n+1]; // +1 to accommodate new element
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    
    scanf("%d %d", &pos, &element);
    
    // Shift elements to the right
    for(int i = n; i >= pos; i--)
        arr[i] = arr[i-1];
    
    arr[pos-1] = element; // Insert element
    
    n++; // Array size increased
    
    // Print updated array
    for(int i = 0; i < n; i++)
        printf("%d ", arr[i]);
        
    return 0;
}
