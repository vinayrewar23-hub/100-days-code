#include <stdio.h>

int main() {
    int a, b, max, lcm;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    // The greater number among a and b
    max = (a > b) ? a : b;

    // Keep increasing max until it's divisible by both
    while (1) {
        if (max % a == 0 && max % b == 0) {
            lcm = max;
            break;
        }
        max++;
    }

    printf("LCM of %d and %d = %d\n", a, b, lcm);

    return 0;
}
