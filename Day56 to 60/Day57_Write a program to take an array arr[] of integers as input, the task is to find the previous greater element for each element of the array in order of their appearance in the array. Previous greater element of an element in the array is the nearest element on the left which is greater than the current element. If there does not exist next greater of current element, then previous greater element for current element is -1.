#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    for(int i = 0; i < n; i++) {
        int prevGreater = -1;

        // Check all elements on the left
        for(int j = i - 1; j >= 0; j--) {
            if(arr[j] > arr[i]) {
                prevGreater = arr[j];
                break;   // nearest greater found
            }
        }

        printf("%d", prevGreater);
        if(i != n - 1) printf(",");   // comma separated output
    }

    return 0;
}
