#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int nums[n], ans[n];

    for (int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    // Step 1: Prefix products
    ans[0] = 1;
    for (int i = 1; i < n; i++)
        ans[i] = ans[i - 1] * nums[i - 1];

    // Step 2: Multiply suffix products
    int suffix = 1;
    for (int i = n - 1; i >= 0; i--) {
        ans[i] *= suffix;
        suffix *= nums[i];
    }

    // Print answer array
    for (int i = 0; i < n; i++) {
        printf("%d", ans[i]);
        if (i != n - 1) printf(" ");
    }

    return 0;
}

