#include <stdio.h>

int main() {
    int i;

    // First group: 1 star
    for (i = 1; i <= 1; i++) {
        printf("*\n");
    }
    printf("\n");

    // Second group: 4 stars
    for (i = 1; i <= 4; i++) {
        printf("*\n");
    }
    printf("\n");

    // Third group: 3 stars
    for (i = 1; i <= 3; i++) {
        printf("*\n");
    }
    printf("\n");

    // Fourth group: 1 star
    for (i = 1; i <= 1; i++) {
        printf("*\n");
    }

    return 0;
}
